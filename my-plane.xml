<?xml version="1.0"?>
<Container version="2">
  <Name>Plane</Name>
  <Repository>makeplane/plane-aio-community:stable</Repository>
  <Registry>https://hub.docker.com/r/makeplane/plane-aio-community</Registry>
  <Network>icemulnet</Network>
  <MyIP>172.18.0.26</MyIP>
  <Shell>bash</Shell>
  <Privileged>false</Privileged>
  <Support>https://docs.plane.so/</Support>
  <Project>https://plane.so/</Project>
  <ReadMe/>
  <Overview>Plane is an open-source software development tool. This All-in-One image bundles the Plane application services (Web, API, Worker) into a single container.</Overview>
  <Category>Tools: ProjectManagement:</Category>
  <WebUI>http://[IP]:8083/</WebUI>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/walkxcode/dashboard-icons/master/png/plane.png</Icon>
  <ExtraParams>--restart unless-stopped --label "net.unraid.docker.folder=Plane" --label "net.unraid.docker.managed=dockerman"</ExtraParams>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1772134800</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="Web Port" Target="80" Default="8083" Mode="tcp" Description="The port to access the Plane WebUI." Type="Port" Display="always" Required="true" Mask="false">8083</Config>
  <Config Name="Domain Name" Target="DOMAIN_NAME" Default="192.168.128.43" Mode="" Description="IP or FQDN only (no port)." Type="Variable" Display="always" Required="true" Mask="false">192.168.128.43</Config>
  <Config Name="Database URL" Target="DATABASE_URL" Default="postgresql://plane:plane@172.18.0.20/plane" Mode="" Description="Internal Postgres URL." Type="Variable" Display="advanced" Required="true" Mask="false">postgresql://plane:plane@172.18.0.20/plane</Config>
  <Config Name="Redis URL" Target="REDIS_URL" Default="redis://172.18.0.21:6379/0" Mode="" Description="Internal Redis URL." Type="Variable" Display="advanced" Required="true" Mask="false">redis://172.18.0.21:6379/0</Config>
  <Config Name="AMQP URL" Target="AMQP_URL" Default="amqp://plane:plane@172.18.0.22:5672/plane" Mode="" Description="Internal RabbitMQ URL." Type="Variable" Display="advanced" Required="true" Mask="false">amqp://plane:plane@172.18.0.22:5672/plane</Config>
  <Config Name="Secret Key" Target="SECRET_KEY" Default="plane-secret-key-12345" Mode="" Description="" Type="Variable" Display="advanced" Required="true" Mask="false">plane-secret-key-12345</Config>
  <Config Name="AWS Region" Target="AWS_REGION" Default="us-east-1" Mode="" Description="MinIO region." Type="Variable" Display="advanced" Required="true" Mask="false">us-east-1</Config>
  <Config Name="AWS Access Key" Target="AWS_ACCESS_KEY_ID" Default="access-key" Mode="" Description="MinIO access key." Type="Variable" Display="advanced" Required="true" Mask="false">access-key</Config>
  <Config Name="AWS Secret Key" Target="AWS_SECRET_ACCESS_KEY" Default="secret-key" Mode="" Description="MinIO secret key." Type="Variable" Display="advanced" Required="true" Mask="false">secret-key</Config>
  <Config Name="S3 Endpoint" Target="AWS_S3_ENDPOINT_URL" Default="http://172.18.0.23:9000" Mode="" Description="Internal MinIO endpoint URL." Type="Variable" Display="advanced" Required="true" Mask="false">http://172.18.0.23:9000</Config>
  <Config Name="S3 Bucket" Target="AWS_S3_BUCKET_NAME" Default="uploads" Mode="" Description="MinIO bucket name for uploads." Type="Variable" Display="advanced" Required="true" Mask="false">uploads</Config>
  <Config Name="File Size Limit" Target="FILE_SIZE_LIMIT" Default="5242880" Mode="" Description="Max upload size in bytes (default 5MB)." Type="Variable" Display="advanced" Required="false" Mask="false">5242880</Config>
  <Config Name="External Port" Target="APP_PORT" Default="8083" Mode="" Description="Host port Plane is exposed on. Used to build correct auth redirect URLs." Type="Variable" Display="always" Required="true" Mask="false">8083</Config>
  <Config Name="SMTP Host" Target="EMAIL_HOST" Default="smtp.gmail.com" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">smtp.gmail.com</Config>
  <Config Name="SMTP Port" Target="EMAIL_PORT" Default="587" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">587</Config>
  <Config Name="SMTP User" Target="EMAIL_HOST_USER" Default="icemul@gmail.com" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">icemul@gmail.com</Config>
  <Config Name="SMTP Password" Target="EMAIL_HOST_PASSWORD" Default="rrmncwhyppheilin" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="true">rrmncwhyppheilin</Config>
  <Config Name="SMTP From Email" Target="EMAIL_FROM" Default="icemul@gmail.com" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">icemul@gmail.com</Config>
  <Config Name="SMTP Use TLS" Target="EMAIL_USE_TLS" Default="1" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">1</Config>
  <Config Name="Enable SMTP" Target="ENABLE_SMTP" Default="1" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">1</Config>
  <Config Name="Start Script" Target="/app/start.sh" Default="/mnt/user/appdata/plane/start.sh" Mode="ro" Description="Patched start.sh that adds port to WEB_URL/CORS. Do not remove." Type="Path" Display="advanced" Required="true" Mask="false">/mnt/user/appdata/plane/start.sh</Config>
</Container>
